    "N=len(ABC)\n",
    "G={}\n",
    "for i in range(1,N+1):\n",
    "    G[i]=ACC(ABC,i)\n",
    "    G[i]=set(G[i])\n",
    "    print(G[i])\n",
    "    print()\n"
