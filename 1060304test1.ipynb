   "source": [
    "n = input() #輸入一個正整數\n",
    "k = 0 # \n",
    "L = 0 # \n",
    "for i in range(0,len(n),2):\n",
    "    k=k +int(n[i]) #與最高位數同為(奇或偶)位數之總和\n",
    "for j in range(1,len(n),2):\n",
    "    L=L+int(n[j])  #與最高位數不同(奇或偶)位數之總和\n",
    "\n",
    "s =abs(k-L) #兩數相減取絕對值\n",
    "print(s)"
   ]
